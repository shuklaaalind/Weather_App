{
  "name": "rnstarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "resetIos": "cd ios && pod deintegrate && rm -rf Podfile.lock && pod install && cd .. && yarn run ios",
    "resetAndroid": "cd android && ./gradlew clean && cd .. && yarn run android",
    "postinstall": "yarn rnuc .env && npx pod-install",
    "build-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && yarn run android",
    "postuninstall": "yarn rnuc .env && npx pod-install"
  },
  "dependencies": {
